{
    "name": "paintover",
    "manifest_version": 2,
    "version": "0.1",

    "description": "paintover - description",
    "background": {
        "scripts": ["scripts/background.js"]
    },
    "page_action": {
        "default_icon": "assets/icon.png",
        "default_title": "paintover - default title"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "storage",
        "webRequest",
        "*://*.google.com/"
    ],
    "options_page": "views/options.html",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": ["vendor/jquery/jquery.min.js","scripts/popover.js","scripts/contentscript.js"],
            "css": ["styles/content.css"],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "web_accessible_resources" :[
      "views/vocaReg.html"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' ",
    "icons": {
        "16": "assets/icon.png",
        "128": "assets/icon.png"
    }
}
